{
  "manifest_version": 3,
  "name": "URL-Threat-Analyzer-With-Extension",
  "version": "1.0",
  "description": "Cybersecurity tool that detects whether a URL is benign or malicious using a KNN model with 96.77% accuracy."

  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ]
}
